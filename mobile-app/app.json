{
  "expo": {
    "name": "Ariss",
    "slug": "mobile-app",
    "version": "1.0.2",
    "scheme": "mobile-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/1.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/Ariss_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nxtribe.ariss",
      "buildNumber": "1.0.2",
      "infoPlist": {
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to select images.",
        "NSCameraUsageDescription": "We need access to your camera to take pictures.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/1.png",
        "backgroundColor": "#000000"
      },
      "package": "com.mujahid_patel.mobileapp"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ca2edbfb-a45e-4ff9-8625-561ab25dee42"
      }
    }
  }
}
